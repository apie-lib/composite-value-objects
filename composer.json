{
  "name": "apie/composite-value-objects",
  "description": "Composer package of the apie library: composite value objects",
  "type": "library",
  "license": "MIT",
  "abandoned": "Code is available in apie/core now",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\CompositeValueObjects\\": "src/"
    }
