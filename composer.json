{
    "name": "apie/composite-value-objects",
    "description": "Traits to build Apie-compatible value objects that are basically a composite of primitives/other value objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Jordaan",
            "email": "pieter_jordaan@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Apie\\CompositeValueObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apie\\Tests\\CompositeValueObjects\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "apie/core": "dev-main",
        "apie/type-juggling": "dev-main",
        "apie/value-objects": "dev-main",
        "phpdocumentor/reflection-docblock": "^4.3|5.*",
        "symfony/polyfill-php80": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
